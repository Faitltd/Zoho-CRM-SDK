{
  "name": "@yourcompany/zoho-crm",
  "version": "0.1.0",
  "description": "Modern Zoho CRM SDK for Node.js and TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./leads": {
      "types": "./dist/modules/leads.d.ts",
      "import": "./dist/modules/leads.js",
      "require": "./dist/modules/leads.cjs"
    },
    "./contacts": {
      "types": "./dist/modules/contacts.d.ts",
      "import": "./dist/modules/contacts.js",
      "require": "./dist/modules/contacts.cjs"
    },
    "./deals": {
      "types": "./dist/modules/deals.d.ts",
      "import": "./dist/modules/deals.js",
      "require": "./dist/modules/deals.cjs"
    },
    "./webhooks": {
      "types": "./dist/modules/webhooks.d.ts",
      "import": "./dist/modules/webhooks.js",
      "require": "./dist/modules/webhooks.cjs"
    },
    "./bulk": {
      "types": "./dist/modules/bulk.d.ts",
      "import": "./dist/modules/bulk.js",
      "require": "./dist/modules/bulk.cjs"
    },
    "./experimental": {
      "types": "./dist/experimental/index.d.ts",
      "import": "./dist/experimental/index.js",
      "require": "./dist/experimental/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "SECURITY.md",
    "LICENSE",
    "docs"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "api:prepare": "tsc -p tsconfig.api.json",
    "api:extract": "npm run api:prepare && api-extractor run --local",
    "api:baseline": "npm run api:extract && node scripts/semver/update-api-baseline.cjs",
    "deprecations:scan": "node scripts/deprecation-analyzer.cjs",
    "migrate:v1-to-v2": "node scripts/migrate/cli.cjs v1-to-v2 ./src",
    "migrate:validate": "node scripts/migrate/validate.cjs ./src",
    "bundle:size": "node scripts/size/bundle-size.cjs",
    "bundle:compare": "node scripts/size/compare-bundle.cjs",
    "bundle:baseline": "node scripts/size/update-baseline.cjs",
    "release": "standard-version",
    "release:beta:major": "npm version premajor --preid=beta",
    "release:beta:minor": "npm version preminor --preid=beta",
    "release:beta:patch": "npm version prepatch --preid=beta",
    "semver:check": "node scripts/semver/verify-version.cjs",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "docs": "typedoc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.43.0",
    "@types/node": "^20.11.30",
    "@biomejs/biome": "^1.9.4",
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "^0.25.0",
    "jscodeshift": "^17.3.0",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.28.16",
    "vitest": "^2.1.8",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "Node",
    "Zoho",
    "CRM",
    "OAuth2",
    "API",
    "TypeScript"
  ],
  "author": "Zoho CRM API Team (support@zohocrm.com)"
}
